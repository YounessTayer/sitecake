{
	"name": "sitecake/sitecake-cms",
    "description": "SiteCake - a simple, drag and drop CMS",
	"version": "2.2.10",
    "type": "project",
    "keywords": [ "cms" ],
    "homepage": "http://sitecake.com",
    "licence": "GPL-3.0+",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/predragleka/dindent"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-mbstring": "*",
        "silex/silex": "1.2",
        "jdesrosiers/silex-cors-provider": "0.1",
        "symfony/translation": "2.3",
        "symfony/config": "2.3",
        "symfony/yaml": "2.3",
        "pclzip/pclzip": "dev-master#19dd1de9d3f5fc4d7d70175b4c344dee329f45fd",
        "electrolinux/phpquery": "dev-master#aa09e7af2e8d9a965102cf50d9b657aca96fd013",
        "smottt/wideimage": "1.1.1",
        "league/flysystem": "1.0.12",
        "gajus/dindent": "dev-master",
        "psr/log": "~1.0",
        "mheap/Silex-Memcache": "1.*",
        "jrschumacher/symfony-redis-session-handler": "1.*"
    },
    "autoload": {
        "psr-4" : {
            "Sitecake\\": "src/Sitecake"
        }
    },
    "require-dev": {
    	"phpunit/phpunit": "4.3.*",
        "mockery/mockery": "0.9.2",
        "mikey179/vfsStream" : "1.6.2",
        "symfony/browser-kit" : "2.1.2",
        "symfony/css-selector": ">=2.3,<2.6-dev"
    }  
}
